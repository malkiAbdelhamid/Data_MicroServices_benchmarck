{
  "select": {
    "movie_title": "MIN(ms_title.Title.title)"
  },
  "joins": [
    "ms_title.Title.id = ms_movie_companies.MovieCompanies.movieid",
    "ms_company_name.CompanyName.id = ms_movie_companies.MovieCompanies.companyid",
    "ms_title.Title.id = ms_movie_keyword.MovieKeyword.movieid",
    "ms_movie_keyword.MovieKeyword.keywordid = ms_keyword.Keyword.id",
    "ms_movie_companies.MovieCompanies.movieid = ms_movie_keyword.MovieKeyword.movieid"
  ],
  "where": [
    "ms_keyword.Keyword.keyword = 'character-name-in-title'",
    "ms_company_name.CompanyName.country_code = '[us]'"
  ]
}