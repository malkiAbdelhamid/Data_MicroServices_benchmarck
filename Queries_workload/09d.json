{
  "select": {
    "movie_title": "MIN(ms_title.Title.title)"
  },
  "joins": [
    "ms_title.Title.id = ms_movie_companies.MovieCompanies.movieid",
    "ms_company_name.CompanyName.id = ms_movie_companies.MovieCompanies.companyid"
  ],
  "where": [
    "ms_company_name.CompanyName.country_code = '[us]'"
  ]
}